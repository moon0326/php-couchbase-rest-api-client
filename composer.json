{
    "name": "moon/couchbaserestclient",
    "Description": "Couchbase REST API client in PHP",
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "authors": [
        {
            "name": "moon0326",
            "email": "kmk1986@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/var-dumper": "^3.4",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Moon\\CouchbaseRestClient\\": "./src/"
        }
